class Solution {
public:
    vector<int> plusOne(vector<int>& D)
    {
        for(int i=D.size()-1;i>=0;i--)
        {
            if(D[i]<9)
            {
                D[i]++;
                 return D;
            }
        else
        {
            D[i]=0;
        }
        }
        D.push_back(0);
        D[0]=1;
        return D;
        
    }
};
